<!DOCTYPE html>
<html lang="en">
<style>
    table, th, td {
      border:1px solid black;
    }
</style>
<%- include('../partials/head'); %>

<body class="container">
<body style="background-color: #DBF2FF"
<%- include('../partials/header'); %>

<style>
    .btn {
      background-color: DodgerBlue;
      border: none;
      color: white;
      padding: 12px 16px;
      font-size: 16px;
      cursor: pointer;
    }
    
    /* Darker background on mouse-over */
    .btn:hover {
      background-color: RoyalBlue;
    }
    </style>

<main> 
    <br>
    <div class="jumbotron" style="background-color:#A4BCE0">
      <h6><center><strong>AIRLINE PROSPECT INFORMATION</strong></center></h6>
        <table style="width:100%">
          <tr>
            <th>Prospect ID</th>
            <th>Contact First Name</th>
            <th>Contact Last Name</th>
            <th>Email</th>
            <th>Phone Number</th>
            <th>Airline Name</th>
            <th>Address</th>
            <th>Zip Code</th>
            <th>State</th>
            <th>Country</th>
            <th>Region</th>
            <th>Status</th>
            <th>Start Date</th>
            <th>End Date</th>
          </tr>
          <% report2.forEach(function(report2) { %>
          <tr>
              <td><%= report2.prospect_id %></td>
              <td><%= report2.contact_first_name %></td>
              <td><%= report2.contact_last_name %></td>
              <td><%= report2.contact_email %></td> 
              <td><%= report2.contact_phone_number %></td>  
              <td><%= report2.airline_name %></td>
              <td><%= report2.address %></td> 
              <td><%= report2.zip_code %></td>
              <td><%= report2.state_providence_name %></td>
              <td><%= report2.country_name %></td>
              <td><%= report2.region_name %></td>
              <td><%= report2.status_name %></td>
              <td><%= report2.start_date %></td>
              <td><%= report2.end_date %></td>
          </tr>
          
          <% }); %>
      </table>
  </div>

    
<br>
<ul>
  <a href="/reports"><button class="btn"><i class="fa fa-home"></i> Back</button></a>
</ul>
</main>


</body>
</body>
</html>