<!DOCTYPE html>
<html lang="en">
<%- include('../partials/head'); %>

<body class="container">
<body style="background-color: #000000;"
<%- include('../partials/header'); %>

<style>
    .btn {
      background-color: DodgerBlue;
      border: none;
      color: white;
      padding: 12px 16px;
      font-size: 16px;
      cursor: pointer;
    }
    
    /* Darker background on mouse-over */
    .btn:hover {
      background-color: RoyalBlue;
    }
    </style>

<main> 
    <br>
    <div class="jumbotron" style="background-color:rgb(127, 191, 221)">
        <h6><center><strong>Airline Prospect Information</strong></center></h6>
        <% report2.forEach(function(report2) { %>
        <li>
           <strong> <%= report2.airline_name %> </strong> <%= report2.contact_first_name %> <%= report2.contact_last_name %> <%= report2.contact_email %> <%= report2.contact_phone_number %> <%= report2.address %> <%= report2.zip_code %>
        </li>

        <% }); %>
    </div>

    

<ul>
    <a href="/"><button class="btn"><i class="fa fa-home"></i> Home</button></a>

</main>


</body>
</body>
</html>