<!DOCTYPE html>
<html lang="en">
<style>
  table, th, td {
    border:1px solid black;
  }
</style>

<%- include('../partials/head'); %>

<body class="container">
<body style="background-color: #DBF2FF;"

<%- include('../partials/header'); %>

<style>
    .btn {
      background-color: DodgerBlue;
      border: none;
      color: white;
      padding: 12px 16px;
      font-size: 16px;
      cursor: pointer;
    }
    
    /* Darker background on mouse-over */
    .btn:hover {
      background-color: RoyalBlue;
    }
    </style>

<main> 
    <br>
    <div class="jumbotron" style="background-color:#A4BCE0">
      <h6><center><strong>PROSPECT AND PRODUCT</strong></center></h6>
        <table style="width: 100%;">
          <tr>
              <th>Airline</th>
              <th>Category</th>
              <th>Department</th>
              <th>Employee ID</th>
              <th>First Name</th>
              <th>Last Name</th>
              <th>Product Description</th>
              <th>Product Name</th>
              <th>Product SKU</th>
              <th>Prospect ID</th>
          </tr>
          <% report6.forEach(function(report6) { %>
          <tr>
              <td><%= report6.airline_name %></td>
              <td><%= report6.category %></td>
              <td><%= report6.department_name %></td>
              <td><%= report6.employee_id %></td>
              <td><%= report6.first_name %></td>
              <td><%= report6.last_name %></td> 
              <td><%= report6.product_description %></td> 
              <td><%= report6.product_name %></td>
              <td><%= report6.product_sku %></td> 
              <td><%= report6.prospect_id %></td> 
              
          </tr>
          
          <% }); %>
      </table>
  </div>
    
<br>
<ul>
  <a href="/reports"><button class="btn"><i class="fa fa-home"></i> Back</button></a>
</ul>
</main>


</body>
</body>
</html>