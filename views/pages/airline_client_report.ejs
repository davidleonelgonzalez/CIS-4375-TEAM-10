<!DOCTYPE html>
<html lang="en">
<%- include('../partials/head'); %>

<body class="container">
<%- include('../partials/header'); %>

<style>
    .btn {
      background-color: DodgerBlue;
      border: none;
      color: white;
      padding: 12px 16px;
      font-size: 16px;
      cursor: pointer;
    }
    
    /* Darker background on mouse-over */
    .btn:hover {
      background-color: RoyalBlue;
    }
    </style>

<main> 
    <div class="jumbotron" style="background-color:rgb(127, 191, 221)">
        <h6><center>AIRLINE report1 INFORMATION</center></h6>
        <% report1.forEach(function(report1) { %>
        <li>
            Airline report1s:  <strong> <%= report1.airline_name %> </strong> <%= report1.address %> <%= report1.zip_code %> <%= report1.state_id %> <%= report1.country_id %> <%= report1.region_id %> <%= report1.report1_status_id %>
        </li>

        <% }); %>
    </div>

    

<ul>
    <a href="/"><button class="btn"><i class="fa fa-home"></i> Home</button></a>

</main>



</body>
</html>